<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.gada.travelgada.mapper.MainMapper">

	<select id="getDiary" resultType="com.gada.travelgada.domain.MainVO">
	<![CDATA[
		select diary_id, img_path, text, hashtag, diary_date from diary order by diary_date desc limit 6
	]]>
	</select>

</mapper>